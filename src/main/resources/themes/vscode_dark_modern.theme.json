{
  "name": "VSCode Dark Modern",
  "dark": true,
  "author": "Dinesh Srinivasan",
  "editorScheme": "/themes/vscode_dark_brighter.xml",
  "ui": {
    "*": {
      "background": "#181818",
      "foreground": "#DFDFE0",

      "infoForeground": "#A9A9AA",

      "selectionInactiveBackground": "#555657",
      "selectionBackgroundInactive": "#555657",

      "lightSelectionBackground": "#2B4059",
      "lightSelectionForeground": "#FFFFFF",
      "lightSelectionInactiveBackground": "#555657",
      "lightSelectionInactiveForeground": "#DFDFE0",

      "disabledBackground": "#181818",
      "inactiveBackground": "#181818",

      "disabledForeground": "#737374",
      "disabledText": "#737374",
      "inactiveForeground": "#737374",

      "acceleratorForeground": "#D0D0D9",
      "acceleratorSelectionForeground": "#D0D0D9",

      "borderColor": "#414141",
      "disabledBorderColor": "#414141",

      "focusColor": "#24648D",
      "focusedBorderColor": "#4C8CB5",

      "separatorForeground": "#5A5B5C",
      "separatorColor": "#5A5B5C",
      "lineSeparatorColor": "#5A5B5C",

      "modifiedItemForeground": "#1E4D86"
    },

    "ActionButton": {
      "hoverBackground": "#225DA1",
      "hoverBorderColor": "#225DA1",
      "pressedBackground": "#206DE3",
      "pressedBorderColor": "#206DE3"
    },


    "Button": {
      "startBackground": "#424242",
      "endBackground": "#424242",
      "startBorderColor": "#525252",
      "endBorderColor": "#525252",
      "shadowColor": "#363636",

      "default": {
        "foreground": "#DDE9FA",
        "startBackground": "#206DE3",
        "endBackground": "#1C61CA",
        "startBorderColor": "#6099EC",
        "endBorderColor": "#1C61CA",
        "focusedBorderColor": "#45A1F4",
        "focusColor": "#245F84",
        "shadowColor": "#363636"
      }
    },

    "Borders": {
      "color": "#393A3B",
      "ContrastBorderColor": "#4b4c4d"
    },

    "ComboBox": {
      "nonEditableBackground": "#313131",
      "background": "#313131",
      "modifiedItemForeground": "45A1F4",
      "ArrowButton": {
        "iconColor": "#DBDBDB",
        "background": "#313131",
        "disabledIconColor": "#6B6B6C",
        "nonEditableBackground": "#313131"
      }
    },
    "ComboPopup.border": "1,1,1,1,#FFFFFF12",


    "Component": {
      "errorFocusColor": "#843937",
      "inactiveErrorFocusColor": "#433239",
      "warningFocusColor": "#8F7B2E",
      "inactiveWarningFocusColor": "#434136",
      "iconColor": "#DFDFE0",
      "hoverIconColor": "#FFFFFF"
    },

    "Counter": {
      "background": "#909090",
      "foreground": "#000000"
    },

    "DebuggerPopup.borderColor": "#414141",

    "DefaultTabs": {
      "borderColor": "#484949",
      "background": "#2A2B2B",
      "inactiveUnderlineColor": "#24648D",
      "hoverBackground": "#262727",
      "underlineColor": "#1D7BED",
      "underlineHeight": 2,
      "underlinedTabBackground": "#3D3E3F"
    },

    "DragAndDrop": {
      "areaForeground": "#DFDFE0",
      "areaBackground": "#363737",
      "borderColor": "#414141"
    },

    "Editor": {
      "background": "#1f1f1f",
      "SearchField.background": "#1f1f1f"
    },
    "EditorTabs": {
      "tabInsets": true,
      "underlinedTabBackground": "#1f1f1f",
      "inactiveColoredFileBackground": "#2d281d"
    },

    "FileColor": {
      "Yellow": "#3D3B37",
      "Green": "#383C38",
      "Blue": "#343A3E",
      "Violet": "#3C3A3D",
      "Orange": "#3D3A36",
      "Rose": "#3D383B"
    },

    "Toolbar.Floating.borderColor": "#393A3B",
    "ToolWindow.Header": {
      "borderColor": "#393A3B"
    },

    "Notification": {
      "background": "#1f1f1f",
      "borderColor": "#393A3B"
    },

    "NotificationsToolwindow": {
      "newNotification.background": "#393A3B"
    },

    "SearchMatch": {
      "startBackground": "#f5bc2d",
      "endBackground": "#e9b32b"
    },

    "ToggleButton": {
      "onBackground": "#1D7BED"
    },
    "CompletionPopup": {
      "matchForeground": "#148be8"
    },
    "Popup": {
      "Header": {
        "activeBackground": "#1f1f1f",
        "inactiveBackground": "#4A4E52"
      }
    },

    "SearchEverywhere": {
      "Tab": {
        "selectedForeground": "#F9FCFF",
        "selectedBackground": "#555A5E"
      }
    },
    "ToolWindow": {
      "background": "#181818",
      "Header.borderColor": "#ffffff12"
    },
    "Tree": {
      "background": "#181818"
    },


    "Plugins":{
      "borderColor": "#ffffff12"
    },

    "WelcomeScreen": {
      "background": "#181818",
      "SidePanel.background": "#1e1e1e",
      "borderColor": "#ffffff12",
      "Projects.background": "#181818",
      "Projects.actions.background": "#225DA1"
    }
  }
}
